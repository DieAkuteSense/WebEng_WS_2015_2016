<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>TeacherServlet</servlet-name>
        <servlet-class>de.dhbw.de.webeng.TeacherServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HelloWorldServlet</servlet-name>
        <servlet-class>de.dhbw.de.webeng.HelloWorldServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActionServlet</servlet-name>
        <servlet-class>de.dhbw.de.webeng.ActionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HeadersServlet</servlet-name>
        <servlet-class>de.dhbw.de.webeng.HeadersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SchoolClassServlet</servlet-name>
        <servlet-class>de.dhbw.de.webeng.SchoolClassServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentServlet</servlet-name>
        <servlet-class>de.dhbw.de.webeng.StudentServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>TeacherServlet</servlet-name>
        <url-pattern>/teacher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloWorldServlet</servlet-name>
        <url-pattern>/helloworld</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActionServlet</servlet-name>
        <url-pattern>/action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HeadersServlet</servlet-name>
        <url-pattern>/headers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SchoolClassServlet</servlet-name>
        <url-pattern>/schoolclass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentServlet</servlet-name>
        <url-pattern>/student</url-pattern>
    </servlet-mapping>
</web-app>